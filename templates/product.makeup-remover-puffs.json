      "blocks": {
        "card_thQbK4": {
          "type": "card",
          "settings": {
            "title": "Can the makeup remover puffs be used on sensitive skin?",
            "content": "Yes, the helloskin original makeup remover puffs are gentle and perfect for all skin types, including sensitive skin. They effectively remove makeup without causing irritation or disrupting your skin barrier."
          }
        },
        "card_nQd7dG": {
          "type": "card",
          "settings": {
            "title": "How often should I wash the makeup remover puffs?",
            "content": "Rinse the puff thoroughly after each use and hang it to dry. For a deeper clean, machine wash them in the helloskin washbag once a week or as needed."
          }
        },
        "card_QeKdF7": {
          "type": "card",
          "settings": {
            "title": "Do I need to use a cleanser with the puffs?",
            "content": "No cleanser is neededâ€”just water! However, you can follow up with a cleanser if desired for an extra-clean feeling."
          }
        },
        "card_pCcCWD": {
          "type": "card",
          "settings": {
            "title": "How long do the makeup remover puffs last?",
            "content": "With proper care, the puffs can last up to 300 washes or more before they should be replaced for hygiene reasons."
          }
        },
        "card_rKJppp": {
          "type": "card",
          "settings": {
            "title": "Are the puffs eco-friendly?",
            "content": "Yes! The puffs are reusable and reduce the need for disposable wipes, making them a sustainable and eco-conscious choice for makeup removal."
          }
        },
        "card_tUbbg8": {
          "type": "card",
          "settings": {
            "title": "Do I need to use a makeup remover product like micellar water with the puffs?",
            "content": "No, just water! In fact, we advise against using micellar water or similar products, as they often contain ingredients that can strip your skinâ€™s natural moisture barrier over time and may contribute to congestion or acne."
          }
        },
        "card_tW7PGU": {
          "type": "card",
          "settings": {
            "title": "How fast do you deliver?",
            "content": "All orders placed before 2PM Monday-Friday are shipped same day from our local warehouses based in Australia, NZ, UAE or US depending where the stock is located."
          }
        }
