    "ss_glow_faq_HGxQzh": {
      "type": "ss-glow-faq",
      "blocks": {
        "card_nQd7dG": {
          "type": "card",
          "settings": {
            "title": "Can I use all the products in this bundle on sensitive skin?",
            "content": "Yes, the products in the helloskin signature bundle are designed to be gentle and suitable for most skin types, including sensitive skin. However, we recommend patch testing new products before full use to ensure compatibility with your skin."
          }
        },
        "card_Tqdp4F": {
          "type": "card",
          "settings": {
            "title": "How often should I use the LED handset?",
            "content": "For best results, use the LED handset daily for at least 4-6 weeks. Be consistent with your sessions to target skin concerns like fine lines, breakouts, and dullness effectively."
          }
        },
        "card_pFWX73": {
          "type": "card",
          "settings": {
            "title": "Do I need to replace the gloves or makeup remover puffs?",
            "content": "Yes, for hygiene purposes, we recommend replacing the exfoliating gloves every 6‚Äì12 weeks. They can be composted or responsibly recycled after use. For the makeup remover puffs, they are machine washable and reusable over 300 times but we recommend replacing yearly if necessary."
          }
        },
        "card_LVFKnR": {
          "type": "card",
          "settings": {
            "title": "Can the crystal hair removers be used on sensitive areas?",
            "content": "The crystal hair removers are not recommended for use on sensitive areas such as underarms or the bikini line. For best results, use them on wet skin and always perform a patch test first, especially on sensitive skin. For detailed instructions, refer to the comprehensive guide on the product page or in our support centre."
          }
        },
        "card_tW7PGU": {
          "type": "card",
          "settings": {
            "title": "How fast do you deliver?",
            "content": "All our products are stored in our warehouse and we ship all orders daily."
          }
        }
      },
