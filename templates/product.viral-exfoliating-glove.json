    "ss_glow_faq_HGxQzh": {
      "type": "ss-glow-faq",
      "blocks": {
        "card_thQbK4": {
          "type": "card",
          "settings": {
            "title": "How often should I use the exfoliating glove?",
            "content": "Use the glove 1–2 times a week for best results. Overuse may cause irritation, so let your skin rest between sessions."
          }
        },
        "card_nQd7dG": {
          "type": "card",
          "settings": {
            "title": "Can the exfoliating body glove be used on sensitive skin?",
            "content": "Yes, it’s gentle enough for sensitive skin when used with light pressure. Always test on a small area first and avoid over-exfoliating."
          }
        },
        "card_QeKdF7": {
          "type": "card",
          "settings": {
            "title": "Do I need to use any exfoliating products with the glove?",
            "content": "No additional products are needed! The glove works effectively with just water, eliminating the need for scrubs or chemicals."
          }
        },
        "card_pCcCWD": {
          "type": "card",
          "settings": {
            "title": "How long will the glove last?",
            "content": "With proper care, the glove can last 6–12 weeks. Replace it within this time to maintain hygiene and effectiveness."
          }
        },
        "card_rKJppp": {
          "type": "card",
          "settings": {
            "title": "How do I clean the glove?",
            "content": "Rinse thoroughly after each use and hang to air dry. For a deeper clean, machine wash in a laundry bag when needed."
          }
        },
        "card_tUbbg8": {
          "type": "card",
          "settings": {
            "title": "What are the gloves made from, and are they eco-friendly?",
            "content": "The gloves are made from 100% viscose fabric, which is fully recyclable or compostable, making them an environmentally friendly choice for your skincare routine."
          }
        },
        "card_tW7PGU": {
          "type": "card",
          "settings": {
            "title": "How fast do you deliver?",
            "content": "All orders placed before 2PM Monday-Friday are shipped same day from our local warehouses based in Australia, NZ, UAE or US depending where the stock is located."
          }
        }
      },
